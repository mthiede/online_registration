<h1>Listing Registration Configs</h1>

<table>
<% @registration_configs.each do |registration_config| %>
  <tr>
    <td><%= image_tag("google_webmaster_tools.png") %></td>
    <td><%=h registration_config.name %></td>
    <td><%= link_to 'Show', registration_config %></td>
    <td><%= link_to 'Edit', edit_registration_config_path(registration_config) %></td>
    <td><%= link_to 'Try It!', :controller => :registration, :action => :register, :id => registration_config.id %></td>
    <td><%= link_to 'Destroy', registration_config, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Registration Config', new_registration_config_path %>
